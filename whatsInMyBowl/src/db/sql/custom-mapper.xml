<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>

	<entry key="selectListCount">
		SELECT
		       COUNT(*) count
		  FROM ITEM
	</entry>

	<entry key="selectItemList">
		SELECT *
		  FROM ITEM
		 WHERE CI_SHOW = 'Y'
		   AND CI_STOCK > 0
	</entry>

</properties>